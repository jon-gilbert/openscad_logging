MATCH_TEXT="warning"
NOMATCH_TEXT="INFO: "

SCAD=`cat <<EOM
    include <logging.scad>
    LOG_LEVEL=LOG_WARNING;
    d = log_debug("debug");
    i = log_info("info");
    w = log_warning("warning");
    e = log_error("error");
    //f = log_fatal("fatal");
    assert(d == undef);
    assert(i == undef);
    assert(w == "warning");
    assert(e == "error");
    //assert(f == "fatal");
EOM
`

